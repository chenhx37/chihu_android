<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.heinzchen.chihu">

    <application
        android:name="CApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">

        <activity android:name=".SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".customer.main.MainActivity" />

        <activity android:name=".register.RegisterCustomerActivity" />

        <activity android:name=".login.LoginActivity"></activity>

        <activity android:name=".customer.dishes.DishesActivity" />
        <activity android:name=".customer.order.MakeOrderActivity"/>

        <activity android:name=".provider.ProviderMainActivity" />

        <activity android:name=".provider.canteen.CanteenDetailActivity" />
        <activity android:name=".provider.canteen.AddDishActivity" />
        <activity android:name=".provider.canteen.AddCanteenActivity" />


    </application>


    <uses-permission android:name="android.permission.INTERNET" />
</manifest>
